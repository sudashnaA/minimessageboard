<html>
  <head>
    <link rel="stylesheet" href="/style.css">
    <title><%= title %></title>
  </head>
  <body>
    <div id="container2">
        <div id="titlecontainer">
            <h1><%= title %></h1>
            <p><a href="/">Return</a></p>
        </div>
        <div id="messagecontainer">
            <ul>
                <li><strong>Text:</strong> <%= message.text %></li>
                <li><strong>User:</strong> <%= message.username %></li>
                <li><strong>Date:</strong> <%= message.timestamp %></li>
            </ul>
        </div>
    </div>
      
  </body>
</html>
